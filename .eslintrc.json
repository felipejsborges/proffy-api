{
	"env": {
		"es2020": true,
		"node": true,
		"jest": true
	},
	"extends": [
		"eslint:recommended",
		"prettier",
		"prettier/@typescript-eslint",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"prettier/prettier": ["error"]
	}
}
