		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^26.4.2",
		"prettier": "2.1.1",
		"prettier-eslint": "^11.0.0",
		"ts-jest": "^26.3.0",
		"ts-node-dev": "^1.0.0-pre.56",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"knex": "^0.21.2",
